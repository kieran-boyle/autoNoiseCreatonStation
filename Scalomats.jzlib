if (Scales.selection==12) scale={0, 1, 4, 5, 7, 8, 10};
if (Scales.selection==13) scale={0, 2, 4, 6, 8, 10};
if (Scales.selection==14) scale={0, 2, 3, 5, 6, 8, 9, 11};
